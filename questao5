package trabalhosenai;
import java.util.Scanner;

public class questao5 {
	public static void main (String[] args) {
		Scanner analise = new Scanner (System.in);
		int n1, n2, n3, n4, n5, n6, n7, n8, n9, n10, soma;
		
		System.out.println("Digite o primeiro número: ");
		n1 = analise.nextInt();
		
		System.out.println("Digite o segundo número: ");
		n2 = analise.nextInt();
		
		System.out.println("Digite o terceiro número: ");
		n3 = analise.nextInt();
		
		System.out.println("Digite o quarto número: ");
		n4 = analise.nextInt();
		
		System.out.println("Digite o quinto número: ");
		n5 = analise.nextInt();
		
		System.out.println("Digite o sexto número: ");
		n6 = analise.nextInt();
		
		System.out.println("Digite o setimo número: ");
		n7 = analise.nextInt();
		
		System.out.println("Digite o oitavo número: ");
		n8 = analise.nextInt();
		
		System.out.println("Digite o nono número: ");
		n9 = analise.nextInt();
		
		System.out.println("Digite o decimo número: ");
		n10 = analise.nextInt();
		
		soma = n1+n2+n3+n4+n5+n6+n7+n8+n9+n10;
		System.out.println("A soma dos números lidos é: "+soma);
		
	}

}
